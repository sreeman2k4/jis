<%layout("./layouts/boilerplate")%>
<div class="container1" style="height:100px;border: 1px solid #000;">
    <div class="row" >
        <div class="col" >
            <div class="navbar">
                <details>
                    <summary></summary>
                    <nav class="menu">
                      <a href="/cases">Home</a>
                      <%if(user.isRegistrar=="true"){%><a href="/cases/add">add case</a>
                      <%}%>
                      <a href="/cases/pending">pending cases</a>
                      <a href="/signup">register</a>
                      <a href="/logout">logout</a>
                    </nav>
                  </details>
            </div>
        </div>
        <div class="col">
            <h1 style="color: aliceblue;text-align: center;">JUDICIARY INFORMATION</h1>
        </div>
        <div class="col">
            <div class="container">
                <div class="box">
                  <form class="search" action="/cases/search" method="GET">
                    <input class="input" type="search" name="dsearch" placeholder="Search" aria-label="Search">
                    <button class="searchButton" type="submit"><i class="fa fa-search"></i></button>
                  </form>
                </div>
              </div>
        </div>
    </div>
</div>
<!-- ........................................................................................................................... -->

<div class="container">
    <div class="row">
        <div class="col">
            <div class="wrap animate pop">
                <div class="overlay">
                  <div class="overlay-content animate slide-left delay-2">
                    <h1 class="animate slide-left pop delay-4"><h1><%=incident.CIN%></h1></h1>
                    <p class="animate slide-left pop delay-5" style="color: white; margin-bottom: 2.5rem;"><%=incident.crimeType%>:<%=incident.caseTitle%> <em></em></p>
                  </div>
                  <div class="image-content animate slide delay-5"></div>
                  <div class="dots animate">
                    <div class="dot animate slide-up delay-6"></div>
                    <div class="dot animate slide-up delay-7"></div>
                    <div class="dot animate slide-up delay-8"></div>
                  </div>
                </div>
                <div class="text">
                  <p><img class="inset" src="data:image/jpeg;base64," alt="" /><p><%=incident.caseTitle%></p>
                    <p><b>CIN :</b> <%=incident.CIN%></p>
                    <p><b>creation date:</b><%=incident.dateOfCreation%></p></p>
                    <p><b>Defendent Name:</b><%=incident.defendentName%></p>
                    <p><b>Defendent Address:</b><%=incident.defendentAddress%></p>
                    <p><b>Type of crime:</b><%=incident.crimeType%></p>
                    <p><b>Date of crime:</b><%=incident.committedDate%></p>
                    <p><b>Location of crime:</b><%=incident.committedLocation%></p>
                    <p><b>Arresting officer:</b><%=incident.arrestingOfficer%></p>
                    <p><b>Date of arrest:</b><%=incident.dateOfArrest%></p>
                    <p><b>Presiding judge:</b><%=incident.presidingJudge%></p>
                    <p><b>Public Prosecutor:</b><%=incident.publicProsecutor%></p>
                    <p><b>Date of hearing:</b><%=incident.dateOfHearing%></p>
                    <p><b>Date of completion:</b><%=incident.completionDate%></p>
                    <p><b>Breif details:</b><%=incident.breif%></p>
                    <p><b>Status:</b><%=incident.status%></p></p>
                </div>
              </div>
        </div>
        <!-- ............................................................................................................ -->
        <div class="col">
          <%if(user.isRegistrar=="true"){%>
            <label class="dropdown"  style="position: relative;">
              <div class="dd-button">
                options
              </div>  
              <input type="checkbox" class="dd-input" id="test">
              <ul class="dd-menu">
                <li><a href="/cases/<%=incident._id%>/edit" class="btn btn-primary btn-sm">edit</a></li>
                <li><form action="/cases/<%=incident._id%>?_method=DELETE" method="POST">
                  <button class="delete-button" style="border:none;padding:10px;padding-right: 100px;text-align: center;background-color: red;color:white">delete-case</button>
                </form>
                </li>
                <li><a href="/cases/<%=incident._id%>/addsession">add session</a></li>
                <li class="divider"></li>
                <li>
                  <a href="/cases/find">find a case</a>
                </li>
              </ul> 
            </label>
            <%}%>
        </div>
        <div class="col">
           
                <div class="sessions-container">
                    <% for(let session of incident.sessions){ %>
                      <div class="row">
                        <div class="card">
                          <h4><b>JUDGE: </b><%= session.attendingJudge %></h4>
                          <p>Summary: <%= session.summary %></p>
                          <p><b>Next Date of Hearing: </b><%= session.nextHearingDate %></p>
                          <p><b>date of creation: </b><%= session.dateOfCreation %></p>
                          <%if(user.isRegistrar=="true"){%>
                            <a href="/cases/<%= incident._id %>/edit/<%= session._id %>" class="btn btn-primary btn-sm">Edit</a>
                            <%}%>
                        </div>
                      </div>
                    <% } %>
                  </div>
        </div>
    </div>
</div>
<!-- ..................................................................................................................... -->
<%if(incident.assigned=="not assigned"){%>
  <% if(user.isLawyer=="true"){%>
    <a  href="/cases/<%=incident._id%>/assign">assign</a>
  <%}%>
<%}%>


